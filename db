CREATE TABLE `pedagang` (
  `id` int(11) NOT NULL,
  `kode` varchar(8) NOT NULL,
  `nama` varchar(30) NOT NULL,
  `produk` varchar(50) NOT NULL,
  `jp` varchar(50) NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

